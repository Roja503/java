import java.util.*;
class Main
{
    public static void main(String[] args)
    {
        int n;
        Scanner sc=new Scanner(System.in);
        n=sc.nextInt();
        int[] array=new int[10];
        for (int i=0;i<n;i++)
        {
            array[i]=sc.nextInt();
        }
        System.out.println("array elements are");
        for(int i=0;i<n;i++)
        {
            System.out.println(array[i]);
        }
    }
}
